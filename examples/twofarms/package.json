{
    "name": "@twofarmql/farm-example",
    "version": "0.1.0",
    "private": true,
    "main": "dist/index.js",
    "scripts": {
        "start": "node dist/index.js",
        "build": "tsc",
        "test": "vitest --passWithNoTests --run --hookTimeout=300000",
        "generate": "ts-node deployment.ts",
        "deploy": "npm run generate && cd generated && docker-compose up",
        "deploy:build": "npm run generate && cd generated && docker-compose up --build",
        "deploy:down": "cd generated && docker-compose down",
        "deploy:clean": "cd generated && docker-compose down -v && cd .. && rm -rf generated"
    },
    "dependencies": {
        "@meshobj/cli": "workspace:*",
        "@meshobj/deploy": "workspace:*",
        "axios": "^1.6.7",
        "js-yaml": "^4.1.0"
    },
    "devDependencies": {
        "@meshobj/graphlette": "workspace:^",
        "@types/node": "^20.0.0",
        "openapi-client-axios": "^7.5.5",
        "testcontainers": "^10.18.0",
        "typescript": "^5.0.0",
        "vitest": "^3.0.5"
    }
}
