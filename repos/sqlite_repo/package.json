{
    "name": "@meshobj/sqlite_repo",
    "version": "0.2.0",
    "type": "commonjs",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "publishConfig": {
        "access": "public"
    },
    "exports": {
        ".": "./dist/index.js"
    },
    "description": "SQLite3 Repository for MeshQL",
    "author": "Tom Marsh",
    "license": "MIT",
    "dependencies": {
        "@meshobj/auth": "^0.2.0",
        "@meshobj/cert": "^0.2.0",
        "@meshobj/common": "^0.2.0",
        "@meshobj/graphlette": "^0.2.0",
        "@meshobj/server": "^0.2.0",
        "@types/typescript": "^2.0.0",
        "handlebars": "^4.7.8",
        "log4js": "^6.9.1",
        "sqlite": "^5.1.1",
        "sqlite3": "^5.1.7",
        "typescript": "^5.7.2",
        "uuid": "^11.0.5"
    },
    "scripts": {
        "build": "tsc",
        "test": "yarn vitest --run --coverage && yarn test:bdd",
        "test:bdd": "yarn test:bdd:int && yarn test:bdd:e2e && yarn lcov-result-merger 'coverage/lcov-*.info' coverage/lcov.info",
        "test:bdd:e2e": "c8 --report-dir=coverage --reporter=lcov --all=false --exclude cucumber.js --exclude 'test/**' cucumber-js -p e2e && mv coverage/lcov.info coverage/lcov-e2e.info",
        "test:bdd:int": "c8 --report-dir=coverage --reporter=lcov --all=false --exclude cucumber.js --exclude 'test/**' cucumber-js -p int && mv coverage/lcov.info coverage/lcov-int.info"
    },
    "devDependencies": {
        "@cucumber/cucumber": "^12.2.0",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/node": "^22.10.6",
        "axios": "^1.6.7",
        "c8": "^10.1.3",
        "chai": "^4.3.10",
        "js-yaml": "^4.1.0",
        "jsonwebtoken": "^9.0.2",
        "lcov-result-merger": "^5.0.1",
        "openapi-client-axios": "^7.5.5",
        "ts-node": "^10.9.2",
        "vitest": "^2.1.8"
    },
    "gitHead": "b6648bde930e8a322c8d1a966e152bc83fa0ad31"
}
